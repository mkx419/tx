{
  "name": "@mkx419/rollup-plugin-tx",
  "version": "0.3.0",
  "description": "Rollup plugin for tx",
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "tailwindcss",
    "tx"
  ],
  "homepage": "https://github.com/mkx419/tx",
  "bugs": {
    "url": "https://github.com/mkx419/tx/issues",
    "email": "mkx419@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "mkx419",
    "email": "mkx419@gmail.com"
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkx419/tx.git",
    "directory": "packages/rollup"
  },
  "scripts": {
    "build": "tsup",
    "version": "pnpm version"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "pnpm": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "rollup": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "dependencies": {
    "@mkx419/tx": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^22.7.3",
    "rollup": "^4.22.4"
  }
}
