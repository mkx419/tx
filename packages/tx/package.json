{
  "name": "@mkx419/tx",
  "version": "0.4.0",
  "description": "Simple and useful utility for Tailwind CSS",
  "keywords": [
    "tailwindcss"
  ],
  "homepage": "https://github.com/mkx419/tx",
  "bugs": {
    "url": "https://github.com/mkx419/tx/issues",
    "email": "mkx419@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "mkx419",
    "email": "mkx419@gmail.com"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./transform": {
      "import": {
        "types": "./dist/transform.d.ts",
        "import": "./dist/transform.js"
      },
      "require": {
        "types": "./dist/transform.d.cts",
        "import": "./dist/transform.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkx419/tx.git",
    "directory": "packages/tx"
  },
  "scripts": {
    "build": "tsup",
    "version": "pnpm version"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "pnpm": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@ampproject/remapping": "^2.3.0",
    "@mkx419/tsestree-walker": "^0.1.0",
    "@typescript-eslint/typescript-estree": "^8.8.0",
    "magic-string": "^0.30.11"
  }
}
